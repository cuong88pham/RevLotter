    "helmet": "^3.21.2",
    "meta-auth": "^1.1.2",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
